@{
    "C:\Users\*\AppData\Local\Google\Chrome\User Data\Default\Service Worker\CacheStorage" = @{
        FileNames = @("*")
        RetentionDays = 14
        Depth = 4
    };

    "C:\Users\*\AppData\Local\Google\Chrome\User Data\Default\WebStorage\*\CacheStorage" = @{
        FileNames = @("*")
        RetentionDays = 14
        Depth = 4
    };

    "C:\Users\*\AppData\Local\Google\Chrome\User Data\Default\Cache" = @{
        FileNames = @("*")
        RetentionDays = 14
        Depth = 4
    };

    "C:\Users\*\AppData\Local\Google\Chrome\User Data\Default\Code Cache" = @{
        FileNames = @("*")
        RetentionDays = 14
        Depth = 4
    };

    "C:\Users\*\AppData\Local\Microsoft" = @{
        FileNames = @("*.ost")
        RetentionDays = 365
        Depth = 2
    }
}